{
  "require": {
    "php": ">=8.1",
    "mustangostang/spyc": "^0.6.3",
    "php-di/php-di": "^6.4",
    "monolog/monolog": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "example/App/",
      "Qenary\\": "src/Qenary/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Qenary\\Tests\\": "tests/stubs"}
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "scripts": {
    "test": [
      "composer dump-autoload --optimize --dev",
      "phpunit"
    ]
  }
}
